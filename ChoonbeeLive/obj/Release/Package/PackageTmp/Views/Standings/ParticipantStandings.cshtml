@model IEnumerable<ChoonbeeLive.Models.Standing<ChoonbeeLive.Models.Participant>>

<table>
    <tr>
        <td><b>Participant</b></td>
        <td><b>School</b></td>
        <td><b>Points</b></td>
    </tr>
        
    @foreach(var result in Model){
        <tr>
            <td>@result.MemberKey.FirstName @result.MemberKey.LastName</td>
            <td>@result.MemberKey.School.Name</td>
            <td>@result.Score</td>
        </tr>
    }
</table>